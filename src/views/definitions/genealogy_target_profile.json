          "framework_relationships": { "renderer_type": "comparison_panel", "config": { "left_field": "framework_a", "right_field": "framework_b" } },
          "architectural_summary": { "renderer_type": "prose_block", "config": {} }
        }
      },
      "semantic_constellation": {
        "renderer_type": "nested_sections",
        "sub_renderers": {
          "core_concepts": { "renderer_type": "mini_card_list", "config": { "title_field": "term", "subtitle_field": "role", "badge_field": "centrality" } },
          "concept_clusters": { "renderer_type": "chip_grid", "config": { "label_field": "cluster_name", "count_field": "member_count" } },
