          "vocabulary_signature": { "renderer_type": "chip_grid", "config": { "label_field": "term" } }
        }
      },
      "inferential_commitments": {
        "renderer_type": "nested_sections",
        "sub_renderers": {
          "key_ideas": { "renderer_type": "chip_grid", "config": { "label_field": "idea" } },
          "commitments": { "renderer_type": "mini_card_list", "config": { "title_field": "commitment", "subtitle_field": "type", "badge_field": "strength" } },
          "backings": { "renderer_type": "mini_card_list", "config": { "title_field": "backing", "subtitle_field": "source" } },
          "either_or_choices": { "renderer_type": "comparison_panel", "config": { "left_field": "option_a", "right_field": "option_b" } },
          "hidden_premises": { "renderer_type": "mini_card_list", "config": { "title_field": "premise", "subtitle_field": "why_hidden" } },
          "practical_implications": { "renderer_type": "mini_card_list", "config": { "title_field": "implication", "subtitle_field": "domain" } },
          "argumentative_structure": { "renderer_type": "prose_block", "config": {} }
        }
      }
    }
  },
